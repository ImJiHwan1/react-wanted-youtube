{
  "presets": ["next/babel"],
  "plugins": [
    [
      "module-resolver",
      {
        "root": ["./src"],
        "alias": {
          "@api": "./src/api",
          "@components": "./src/components",
          "@hooks": "./src/hooks",
          "@interfaces": "./src/interfaces",
          "@pages": "./src/pages",
          "@styles": "./src/styles",
          "@store": "./src/store",
          "@images": "./src/images",
          "@util": "./src/util"
        }
      }
    ]
  ]
}
